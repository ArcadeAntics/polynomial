

export(Abel, as.body, as.polynomial, Bessel, Chebyshev1, Chebyshev2,
    ChebyshevT, ChebyshevU, coprime, Cyclotomic, degree, exponents,
    "exponents<-", gcd, Hermite, integral, is.polynomial,
    make.exponents, polynomial, reduce, remove0, ".exponentsPoly<-",
    .polynomial)


S3method(as.body, default)
S3method(as.body, polynomial)
S3method(as.function, polynomial)
S3method(as.list, polynomial)
S3method(as.polynomial, array)
S3method(as.polynomial, character)
S3method(as.polynomial, complex)
S3method(as.polynomial, data.frame)
S3method(as.polynomial, default)
S3method(as.polynomial, integer)
S3method(as.polynomial, logical)
S3method(as.polynomial, matrix)
S3method(as.polynomial, "NULL")
S3method(as.polynomial, numeric)
S3method(as.polynomial, polynomial)
S3method(as.polynomial, vector)
S3method(c, polynomial)
S3method(coef, polynomial)
S3method(degree, polynomial)
S3method(deriv, polynomial)
S3method(exponents, polynomial)
S3method("exponents<-", polynomial)
S3method(integral, polynomial)
S3method(lines, polynomial)
S3method(plot, polynomial)
S3method(points, polynomial)
S3method(predict, polynomial)
S3method(print, polynomial)
S3method(reduce, polynomial)
S3method("-", polynomial)
S3method("*", polynomial)
S3method("/", polynomial)
S3method("[", polynomial)
S3method("[[", polynomial)
S3method("+", polynomial)
